    <LUACode>function onInit()
	super.onInit();
	onValueChanged();	
end	

function onValueChanged()
--	Debug.chat(getValue());
    if getValue() == '' then
        setValue("Equipment");
    end

    getValue()
    if getValue() == 'equipment' then
        window.sub_veh_equip.setVisible(true);
        window.sub_veh_weapon.setVisible(false);
        window.sub_veh_defense.setVisible(false);
    elseif getValue() == 'weapon' then
        window.sub_veh_equip.setVisible(false);
        window.sub_veh_weapon.setVisible(true);
        window.sub_veh_defense.setVisible(false);
    elseif getValue() == 'defense' then
        window.sub_veh_equip.setVisible(false);
        window.sub_veh_weapon.setVisible(false);
        window.sub_veh_defense.setVisible(true);
    end
--		Debug.chat("Return sub Window statuses")
--		Debug.chat(window.sub_veh_equip.isVisible());
--		Debug.chat(window.sub_veh_weapon.isVisible());
--		Debug.chat(window.sub_veh_defense.isVisible());
end</LUACode>
