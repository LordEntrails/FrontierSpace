		end
	else
		for k, v in ipairs(aDoubles) do
			if nResult == v then
				sResult = "[CRITICAL FAIL]";
			end
		end
		if sResult == '' then
			sResult = "[FAIL]";
		end
	end
	--  Check roll result against target and determine if it was a critical
	
	return sResult;
end</Script>
