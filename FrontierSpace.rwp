	local nAdvWillpower = DB.getValue(nodeChar .. ".adv_willpower", 0);

	if nAdvStrength == '1' or nAdvAgility == '1' or nAdvCoordination == '1' or nAdvPerception == '1' or nAdvIntelligence == '1' or nAdvWillpower == '1' then 
