    <LUACode>function action(draginfo)
	local nodeWin = window.getDatabaseNode();
	
	local sName1 = DB.getText(nodeWin, "name", 0);
	local sTradition1 = DB.getText(nodeWin, "tradition", 0);
	local sType1 = DB.getText(nodeWin, "type", 0);
	local sLevel1 = tostring(DB.getValue(nodeWin, "level", 0));
								
		
	local sRequirement1 = "Requirement: " .. DB.getText(nodeWin, "requirement", "-");
	local sTarget1 = "Target/Area: " .. DB.getText(nodeWin, "target", "-");
	local sDuration1 = "Duration: " .. DB.getText(nodeWin, "duration", "-");
	local sTriggered1 = "Triggered: " .. DB.getText(nodeWin, "triggered", "-");
	local sSacrifice1 = "Sacrifice: " .. DB.getText(nodeWin, "sacrifice", "-");
	local sPermanence1 = "Permanence: " .. DB.getText(nodeWin, "permanence", "-");
	local s20plus1 = "20+ result: " .. DB.getText(nodeWin, "twentyplus", "-");
	local sRoll1 = "Roll: " .. DB.getText(nodeWin, "roll", "-");
	local sDamage1 = "Damage: " .. DB.getText(nodeWin, "damage", "-");
																								
	local sDescription1 = "Description:\n" .. DB.getText(nodeWin, "description", "-");

	local sMessage = "**Spell** \nName: " .. sName1 .. "\nTradition: " .. sTradition1 .. "\nType: " .. sType1 .. "\nLevel: " .. sLevel1 .. "\n-------\n" .. sRequirement1 .. "\n" .. sTarget1 .. "\n" .. sDuration1 .. "\n" .. sTriggered1 .. "\n" .. sSacrifice1 .. "\n" .. sPermanence1 .. "\n" .. s20plus1 .. "\n" .. sRoll1 .. "\n" .. sDamage1 .. "\n-------\n" .. sDescription1 .. "\n" ;
	
		msg = {font = "narratorfont", mode = "story"};
		msg.text = sMessage;
		Comm.deliverChatMessage(msg);
	
	return true;
end

function onDragStart(button, x, y, draginfo)
	return action(draginfo);
end

function onDoubleClick(x, y)
	return action();
end</LUACode>
