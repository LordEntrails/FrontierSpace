function onValueChanged()
	local nodeWounds = getDatabaseNode();
	
	local nodeChar = DB.getChild(nodeWounds, "..");
	 -- Gets the parent node (charsheet) one layer up
	 
	local sCTNode = ActorManager.getCTNodeName(nodeChar);
	
	local nValue = DB.getValue(DB.findNode(ActorManager.getCreatureNodeName(nodeChar) .. ".WoundsCurrent"));
	
	RulesetWizard.changeDBValueOOB(DB.findNode((sCTNode .. ".wounds")), nValue);
end
</LUACode>
