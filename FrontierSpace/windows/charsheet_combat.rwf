    <LUACode>
function onValueChanged()
	local nodeBPMax = getDatabaseNode();
	
	local nodeChar = DB.getChild(nodeBPMax, "..");
	 -- Gets the parent node (charsheet) one layer up
	 
	local sCTNode = ActorManager.getCTNodeName(nodeChar);
	
	local nValue = DB.getValue(DB.findNode(ActorManager.getCreatureNodeName(nodeChar) .. ".BodyPointsMax"));
	
	RulesetWizard.changeDBValueOOB(DB.findNode((sCTNode .. ".bptotal")), nValue);
end
</LUACode>
