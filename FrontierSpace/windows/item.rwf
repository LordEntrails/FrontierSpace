    <LUACode>function onLockChanged()
	if sub_gear and sub_gear.subwindow then
		sub_gear.subwindow.update();
	end
	if sub_weapon and sub_weapon.subwindow then
		sub_weapon.subwindow.update();
	end
	if sub_defense and sub_defense.subwindow then
		sub_defense.subwindow.update();
	end
	if sub_scanner and sub_scanner.subwindow then
		sub_scanner.subwindow.update();
	end
	if sub_implant and sub_implant.subwindow then
	sub_implant.subwindow.update();
	end
	
	local bReadOnly = WindowManager.getReadOnlyState(getDatabaseNode());
	name.setReadOnly(bReadOnly);
	cost.setReadOnly(bReadOnly);
	item_type.setReadOnly(bReadOnly);	
																																													
end</LUACode>
